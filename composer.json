{
    "name": "afonsoapsantos/sisweb",
    "description": "Site para gerenciamento",
    "authors": [
        {
            "name": "Afonso Santos",
            "email": "afonsoapsantos@outlook.com"
        }
    ],
    "require": {
        "slim/slim": "^4.9",
        "illuminate/database": "^8.77",
        "slim/psr7": "^1.5",
        "slim/twig-view": "^3.2",
        "php-di/php-di": "^6.3",
        "php": "^8.0",
        "monolog/monolog": "^2.3",
        "psr/log": "1.*",
        "robmorgan/phinx": "^0.12.9",
        "illuminate/pagination": "^8.77"
    },
    "autoload": {
        "psr-4": {
            "Src\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public"
    }
}
